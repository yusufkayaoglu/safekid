<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.24.xsd">

    <changeSet id="004-cleanup-cocuk-konum-columns" author="yusuf">
        <preConditions onFail="MARK_RAN">
            <columnExists tableName="cocuk_konum" columnName="latitude"/>
        </preConditions>
        <dropColumn tableName="cocuk_konum" columnName="latitude"/>
        <dropColumn tableName="cocuk_konum" columnName="longitude"/>
        <dropColumn tableName="cocuk_konum" columnName="optime"/>
    </changeSet>
</databaseChangeLog>
